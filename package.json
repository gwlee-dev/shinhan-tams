{
    "name": "shinhan-tams",
    "version": "1.0.0",
    "description": "Shinhan TAMS",
    "homepage": "https://github.com/gwlee-dev/shinhan-tams#readme",
    "bugs": {
        "url": "https://github.com/gwlee-dev/shinhan-tams/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gwlee-dev/shinhan-tams.git"
    },
    "license": "ISC",
    "author": "Geonwoo Lee",
    "scripts": {
        "build": "npm run clean && parcel build ./src/view/index.pug",
        "clean": "rimraf dist && rimraf ./.parcel-cache",
        "lint": "eslint ./**/*.js --fix && stylelint ./**/*.scss",
        "server": "node ./server.js",
        "dev": "npm run clean && parcel ./src/view/index.pug",
        "start": "nodemon --watch ./package.json --exec npm run dev"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@easepick/amp-plugin": "^1.2.0",
        "@easepick/bundle": "^1.2.0",
        "@parcel/namer-default": "^2.7.0",
        "@parcel/plugin": "^2.7.0",
        "@parcel/transformer-inline-string": "^2.7.0",
        "@parcel/transformer-pug": "^2.7.0",
        "@parcel/transformer-sass": "^2.7.0",
        "@prettier/plugin-pug": "^2.2.0",
        "bootstrap": "^5.2.0",
        "bootstrap-icons": "^1.9.1",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "lodash.escaperegexp": "^4.1.2",
        "nanum-fontface": "^1.0.1",
        "nodemon": "^2.0.20",
        "parcel": "^2.7.0",
        "parcel-namer-custom": "^0.2.0",
        "path-browserify": "^1.0.1",
        "pretendard": "^1.3.5",
        "prettier": "^2.7.1",
        "process": "^0.11.10",
        "rimraf": "^3.0.2",
        "sharp": "^0.29.3",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-recommended-scss": "^7.0.0"
    },
    "parcel-namer-custom": {
        "app.js$": "js/TAMS.[type]",
        "style.scss$": "css/TAMS.[type]",
        ".scss$": "css/[name].[type]",
        ".css$": "css/[name].[type]",
        ".js$": "js/[name].[type]",
        ".ttf$": "fonts/[name].[type]",
        ".woff$": "fonts/[name].[type]",
        ".woff2$": "fonts/[name].[type]",
        ".png$": "image/[name].[type]",
        ".jpg$": "image/[name].[type]",
        ".gif$": "image/[name].[type]",
        ".svg$": "image/[name].[type]"
    },
    "dependencies": {
        "static-server": "^2.2.1"
    }
}
