{
    "name": "shinhan-tams",
    "version": "1.0.0",
    "description": "Shinhan TAMS",
    "homepage": "https://github.com/gwlee-dev/shinhan-tams#readme",
    "bugs": {
        "url": "https://github.com/gwlee-dev/shinhan-tams/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gwlee-dev/shinhan-tams.git"
    },
    "license": "ISC",
    "author": "Geonwoo Lee",
    "scripts": {
        "build": "npm run clean && parcel build ./src/view/index.pug --no-optimize",
        "clean": "rimraf dist && rimraf ./.parcel-cache",
        "lint": "eslint ./**/*.js --fix && stylelint ./**/*.scss",
        "static-server": "node ./static-server.js",
        "server": "babel-node ./server/server.js",
        "dev": "npm run clean && parcel ./src/view/index.pug ",
        "start": "nodemon --watch ./package.json --exec npm run dev",
        "install": "./yarn.cmd install -offline"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@parcel/namer-default": "^2.7.0",
        "@parcel/plugin": "^2.7.0",
        "@parcel/transformer-inline-string": "^2.7.0",
        "@parcel/transformer-pug": "^2.7.0",
        "@parcel/transformer-sass": "^2.7.0",
        "@popperjs/core": "^2.11.6",
        "@prettier/plugin-pug": "^2.2.0",
        "bootstrap": "^5.2.0",
        "bootstrap-icons": "^1.9.1",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "html2pdf": "^0.0.11",
        "html2pdf.js": "^0.10.1",
        "lodash.escaperegexp": "^4.1.2",
        "nanum-fontface": "^1.0.1",
        "nodemon": "^2.0.20",
        "parcel": "^2.7.0",
        "parcel-namer-custom": "^0.2.0",
        "path-browserify": "^1.0.1",
        "pretendard": "^1.3.5",
        "prettier": "^2.7.1",
        "process": "^0.11.10",
        "rimraf": "^3.0.2",
        "sharp": "^0.29.3",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-recommended-scss": "^7.0.0",
        "vanillajs-datepicker": "^1.2.0"
    },
    "parcel-namer-custom": {
        "app.js$": "js/TAMS.[type]",
        "style.scss$": "css/TAMS.[type]",
        ".scss$": "css/[name].[type]",
        ".css$": "css/[name].[type]",
        ".js$": "js/[name].[type]",
        ".ttf$": "fonts/[name].[type]",
        ".woff$": "fonts/[name].[type]",
        ".woff2$": "fonts/[name].[type]",
        ".png$": "image/[name].[type]",
        ".jpg$": "image/[name].[type]",
        ".gif$": "image/[name].[type]",
        ".svg$": "image/[name].[type]"
    },
    "dependencies": {
        "@babel/node": "^7.19.1",
        "@babel/preset-env": "^7.19.3",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "pug": "^3.0.2",
        "static-server": "^2.2.1"
    },
    "alias": {
        "jquery": {
            "global": "$"
        }
    },
    "type": "module"
}
